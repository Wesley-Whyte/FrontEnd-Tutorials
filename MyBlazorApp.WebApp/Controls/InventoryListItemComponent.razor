@if (this.Inventory is not null)
{
    <tr>
        <td>@this.Inventory.Name</td>
        <td>@this.Inventory.Quantity</td>
        <td>@this.Inventory.Price.ToString("c")</td>
        <td><button type="button" class="btn btn-light">Edit</button></td>
    </tr>
}




@code {
    [Parameter]
    public Inventory? Inventory { get; set; }
}
